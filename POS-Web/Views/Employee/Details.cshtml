@model POS_Web.DataContext.EmployeeEntity

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Detail Employee</h2>
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Gender</th>
        <td>@Model.Gender</td>
    </tr>
    <tr>
        <th>Address</th>
        <td>@Model.Address</td>
    </tr>
    <tr>
        <th>City</th>
        <td>@Model.City</td>
    </tr>
    <tr>
        <th>Position</th>
        <td>@Model.Position</td>
    </tr>
</table>
